public static Integer maxSubArraySum(List<Integer> nums, Integer target){
//code here 
    Integer start =0;
    Integer n = nums.size();
    Integer max = -1;
    Integer sum =0;
    for(Integer i =0;i < n;i++)
    {
        sum += nums.get(i);
        while(sum > target)
        {
            sum -= nums.get(start);
            start++;
        }
        if(sum == target)
        {
           max = Math.max(max, i-start+1);
        }   
    }
    return max;
}