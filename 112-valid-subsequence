public Boolean isSubSequence(String s1, String s2){
    //code here
    Integer i = 0, j=0;
    Integer n = s1.length(), m = s2.length();

    while(i < n && j < m)
    {
        if(s1.charAt(i) == s2.charAt(j))
            i++;
        while(s1.charAt(i) != s2.charAt(j))
            j++;
    }
    return i+1  == n;
}